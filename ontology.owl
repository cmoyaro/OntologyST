    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.w3.org/2001/XMLSchema#date -->


    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.owl-ontologies.com/ont#availableIn -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#availableIn">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Transport"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:comment>&quot;Indicates that a transport option is available in a specific city.&quot;</rdfs:comment>
        <rdfs:label>available in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/ont#happensIn -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#happensIn">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Event"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:comment>&quot;Links an event to the city where it occurs.&quot;</rdfs:comment>
        <rdfs:label>happens in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/ont#hasAttraction -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#hasAttraction">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#Attraction"/>
        <rdfs:comment>&quot;Links a city to its tourist attractions.&quot;</rdfs:comment>
        <rdfs:label>has attraction</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/ont#hasEvent -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#hasEvent">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#Event"/>
        <rdfs:comment>&quot;Specifies that a city offers scheduled events.&quot;</rdfs:comment>
        <rdfs:label>has event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/ont#hasHotel -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#hasHotel">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:comment>&quot;Indicates hotels available in a given city.&quot;</rdfs:comment>
        <rdfs:label>has hotel</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/ont#hasRestaurant -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#hasRestaurant">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#Restaurant"/>
        <rdfs:comment>&quot;Indicates restaurants available in a given city.&quot;</rdfs:comment>
        <rdfs:label>has restaurant</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/ont#hasTransport -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#hasTransport">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#Transport"/>
        <rdfs:comment>&quot;Specifies that a city has access to various means of transport.&quot;</rdfs:comment>
        <rdfs:label>has transport</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/ont#hostsEvent -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#hostsEvent">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#Event"/>
        <rdfs:comment>&quot;Specifies that a hotel hosts or organizes events.&quot;</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/ont#locatedIn -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#locatedIn">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Attraction"/>
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Restaurant"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:comment>&quot;Links an entity (e.g. hotel, restaurant, attraction) to the city where it is located.&quot;</rdfs:comment>
        <rdfs:label>located in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.owl-ontologies.com/ont#offersService -->


    <owl:ObjectProperty rdf:about="http://www.owl-ontologies.com/ont#offersService">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:range rdf:resource="http://www.owl-ontologies.com/ont#Restaurant"/>
        <rdfs:comment>&quot;Indicates that a hotel offers access to a restaurant service.&quot;</rdfs:comment>
        <rdfs:label>offers service</rdfs:label>
    </owl:ObjectProperty>
    



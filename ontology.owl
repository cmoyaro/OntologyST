    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.owl-ontologies.com/ont#averageCost -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#averageCost">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>...</rdfs:comment>
        <rdfs:label>averageCost</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#averagePrice -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#averagePrice">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>...</rdfs:comment>
        <rdfs:label>averagePrice</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#company -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#company">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>...</rdfs:comment>
        <rdfs:label>company</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#cuisineType -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#cuisineType">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>...</rdfs:comment>
        <rdfs:label>cuisineType</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#date -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#date">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment>date</rdfs:comment>
        <rdfs:label>date</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#hotelType -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#hotelType">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Bla bla bla</rdfs:comment>
        <rdfs:label>hotelType</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#location -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#location">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>Where are me?</rdfs:comment>
        <rdfs:label>location</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#name -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#name">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Attraction"/>
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#City"/>
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Event"/>
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>What is my name?</rdfs:comment>
        <rdfs:label>name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#pricePerNight -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#pricePerNight">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>Excuse me, Sir,What is the price for 1 night?</rdfs:comment>
        <rdfs:label>pricePerNight</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#rating -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#rating">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Attraction"/>
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Restaurant"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment>My personal score is...</rdfs:comment>
        <rdfs:label>rating</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#stars -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#stars">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
        <rdfs:comment>...</rdfs:comment>
        <rdfs:label>stars</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#transportType -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#transportType">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Transport"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>...</rdfs:comment>
        <rdfs:label>transportType</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://www.owl-ontologies.com/ont#type -->


    <owl:DatatypeProperty rdf:about="http://www.owl-ontologies.com/ont#type">
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Attraction"/>
        <rdfs:domain rdf:resource="http://www.owl-ontologies.com/ont#Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>...</rdfs:comment>
        <rdfs:label>type</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- http://www.owl-ontologies.com/ont#Attraction -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Attraction">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.owl-ontologies.com/ont#hasAttraction"/>
                <owl:someValuesFrom rdf:resource="http://www.owl-ontologies.com/ont#City"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>&quot;A place of interest for tourists or visitors&quot;</rdfs:comment>
        <rdfs:label>Attraction</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Bike -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Bike">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Private"/>
        <rdfs:comment>&quot;Two wheels, zero emissions, and 100% chance of sore legs.&quot;</rdfs:comment>
        <rdfs:label>Bike</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Budget -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Budget">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:comment>&quot;An affordable hotel with basic amenities.&quot;</rdfs:comment>
        <rdfs:label>Budget</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Bus -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Bus">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Public"/>
        <rdfs:comment>&quot;A magical box that stops at every corner and always arrives when you give up.&quot;</rdfs:comment>
        <rdfs:label>Bus</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Business -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Business">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:comment>&quot;A hotel designed for business travelers with work-oriented services.&quot;</rdfs:comment>
        <rdfs:label>Business</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#ChillAndRelax -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#ChillAndRelax">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Restaurant"/>
        <rdfs:comment>&quot;Come for the food, stay because you forgot what day it is. Cozy vibes included.&quot;</rdfs:comment>
        <rdfs:label>ChillAndRelax</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#City -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#City">
        <rdfs:comment>&quot;A large human settlement where attractions, events, and services are offered.&quot;</rdfs:comment>
        <rdfs:label>City</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Cultural -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Cultural">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Event"/>
        <rdfs:comment>&quot;An event showcasing artistic or traditional cultural expression.&quot;</rdfs:comment>
        <rdfs:label>Cultural</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Event -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Event">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.owl-ontologies.com/ont#date"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>&quot;An organized occasion taking place at a specific time and location.&quot;</rdfs:comment>
        <rdfs:label>Event</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#FastFood -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#FastFood">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Restaurant"/>
        <rdfs:comment>&quot;Where flavor meets speed and nutrition negotiates with ketchup.&quot;</rdfs:comment>
        <rdfs:label>FastFood</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Festival -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Festival">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Event"/>
        <rdfs:comment>&quot;A celebration or cultural event, often recurring annually.&quot;</rdfs:comment>
        <rdfs:label>Festival</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Hotel -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Hotel">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.owl-ontologies.com/ont#locatedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.owl-ontologies.com/ont#City"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>&quot;An establishment providing accommodation to travelers and guests.&quot;</rdfs:comment>
        <rdfs:label>Hotel</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Luxury -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Luxury">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Hotel"/>
        <rdfs:comment>&quot;A high-end hotel offering premium services and comfort.&quot;</rdfs:comment>
        <rdfs:label>Luxury</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Metro -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Metro">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Public"/>
        <rdfs:comment>&quot;Underground hope and despair on rails. Mind the gap.&quot;</rdfs:comment>
        <rdfs:label>Metro</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Monument -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Monument">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Attraction"/>
        <rdfs:comment>&quot;A structure erected to commemorate a person or event.&quot;</rdfs:comment>
        <rdfs:label>Monument</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Museum -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Museum">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Attraction"/>
        <rdfs:comment>&quot;A building where historical, scientific, or artistic items are exhibited.&quot;</rdfs:comment>
        <rdfs:label>Museum</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Park -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Park">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Attraction"/>
        <rdfs:comment>&quot;A public green area for recreation.&quot;</rdfs:comment>
        <rdfs:label>Park</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Private -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Private">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Transport"/>
        <rdfs:comment>&quot;Because sometimes you just want to travel with questionable music alone.&quot;</rdfs:comment>
        <rdfs:label>Private</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Public -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Public">
        <rdfs:subClassOf rdf:resource="http://www.owl-ontologies.com/ont#Transport"/>
        <rdfs:comment>&quot;You’ll get there eventually—just not alone.&quot;</rdfs:comment>
        <rdfs:label>Public</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Restaurant -->


    <owl:Class rdf:about="http://www.owl-ontologies.com/ont#Restaurant">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.owl-ontologies.com/ont#locatedIn"/>
                <owl:someValuesFrom rdf:resource="http://www.owl-ontologies.com/ont#City"/>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:comment>&quot;A place where meals are served to customers.&quot;</rdfs:comment>
        <rdfs:label>Restaurant</rdfs:label>
    </owl:Class>
    


    <!-- http://www.owl-ontologies.com/ont#Sports -->



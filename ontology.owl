<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.owl-ontologies.com/ont#"
     xml:base="http://www.owl-ontologies.com/ont"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:mod="https://w3id.org/mod#"
     xmlns:ont="http://www.owl-ontologies.com/ont#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:swrl="http://www.w3.org/2003/11/swrl#"
     xmlns:swrla="http://swrl.stanford.edu/ontologies/3.3/swrla.owl#"
     xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://www.owl-ontologies.com/ont#">
        <dc:creator>Christian Moya Royo</dc:creator>
        <dc:title xml:lang="en">Smart Tourism Ontology</dc:title>
        <terms:abstract xml:lang="en">Ontology for representing concepts and entities related to smart tourism, including hotels, attractions, restaurants, transports and events.</terms:abstract>
        <terms:description xml:lang="en">This ontology models the main components of smart tourism, connecting cities with their services and events through semantic relationships.</terms:description>
        <terms:license xml:lang="en">Creative Commons Attribution 4.0 International (CC BY 4.0)</terms:license>
        <owl:versionInfo>0.0.1</owl:versionInfo>
        <mod:createdWith rdf:resource="https://chowlk.linkeddata.es/"/>
    </owl:Ontology>
    


